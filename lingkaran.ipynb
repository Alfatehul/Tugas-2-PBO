        "import math\n",
        "\n",
        "class Lingkaran:\n",
        "    def __init__(self, radius):\n",
        "        self.radius = radius\n",
        "\n",
        "    def hitung_luas(self):\n",
        "        return math.pi * self.radius ** 2\n",
        "\n",
        "# Objek\n",
        "blt = Lingkaran(7)\n",
        "print(f\"Luas lingkaran: {blt.hitung_luas():.2f}\")\n"
      ]
    }
